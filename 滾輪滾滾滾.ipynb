    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import math\n",
    "\n",
    "from ipywidgets import interact \n",
    "\n",
    "x = np.linspace(0.1,10,100)\n",
    "\n",
    "def draw(L):\n",
    "    y =2*math.pi*((L**2/12+x*x)/(9.8*x))**(0.5)\n",
    "    plt.plot(x,y,lw=5)\n",
    "\n",
    "interact(draw,L=(1.,300.))"
   ]
